	<%= form_tag 'pages/stock', remote: true, :method => 'get', class: "hidden-imp"  do %>
        <%= text_area_tag :base64, '', :id => "upload-meme-stock-field" %>
        <%= text_area_tag :stocktype, 'meme' %>
        <%= submit_tag "SEND", :id => "upload-meme-stock-submit"%>
    <% end %>

	<div id="meme-above">

		<div id="add-meme-url">
			<i class="material-icons" id="upload-meme-stock">add</i>
			<input id="upload-meme-stock-input" placeholder="Paste image address">
		</div>
		<input id="search-memes" placeholder="search memes .." data-search2 >

	</div>

	<% if user_signed_in? %>
	<% @stock1.each do |s| %>
		<div crossorigin="" id="stock-<%= s.id %>" class="pick-meme-container" data-original="<%= s.image.url %>" >
			<i class="material-icons delete-meme-stock delete-stock">clear</i>
			<p class="meme-title" contenteditable="true"><span>Custom</span>

			<%= form_tag 'pages/deleteStock', remote: true, :method => 'get', class: "hidden-imp"    do %>
	            <%= text_area_tag :id, s.id %>
	            <%= submit_tag "SEND"%>
	        <% end %>
		</div>
    <% end %>
    <% end %>
