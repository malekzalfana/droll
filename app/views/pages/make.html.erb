
<body id="<%= controller.action_name %>" class="<%= if user_signed_in? != true then 'new-user' end %> <%= if user_signed_in? &&  current_user.passed.nil? && current_user.posts.count > 0 then "not-passed" end %> <% if user_signed_in? %>signed-in<% end %> <% if user_signed_in? %> signed-in<% else %> REMOVEDnot-signed-in<% end %> <%= if request.original_url.include?('?app=true') then 'app' end %>">
<%= render '/layouts/activities' %>
<%= render '/layouts/pick_trend' %>
<div id="overlay-under-notif"></div>

	<span id="make-logo">/make</span>
	<div id="tabs-wrapper">
		<div id="memes-tab" class="tabs active-tab">Meme</div>
	  <div id="images-tab" class="tabs">GIF</div>
	  <div id="rage-comics-tab" class="tabs">Rage Comic<i class="material-icons hidden">computer</i></div>
	  <div id="video-tab" class="tabs prevent-cursor hidden-imp">Video<span class="coming-soon">coming soon</span></div>
	</div>
	<div id="memes-content" class="choosen-content">


		<div class="middle-column" id="meme-wrapper">
			<%# if user_signed_in? %>
			<%= form_for @post, html: { multipart: true } do |f| %>
			<%= f.text_area :title, id: "created-meme-title", placeholder: "Meme Title " %>
			 <input id="base64" type="hidden" value="" name="base64"/>
			 <div id="image-for-meme">
			 	<p class="add-image-to-meme"><i class="material-icons">add_a_photo</i>Add Top Image</p>
			 	<input placeholder="paste image address.." id="image-for-meme-input" class="hidden-imp"></input>
			 	<i id="image-for-meme-add" class="material-icons hidden-imp">add</i>
			 </div>
			 <i id="image-for-meme-reset" class="material-icons">close</i>
			 <img src="" id="image-for-meme-img">
			 <canvas  class="middle-column" id="meme-canvas" height="1100" width="1100"></canvas>

			 <%= f.text_field :facenumber,id: "created-meme-facenumber-field", class: "hidden anonymous-image-field" %>
			 <%= f.text_field :imageaddress,id: "image-for-meme-field", class: "hidden" %>
			 <%= f.text_field :tag_list, class: "tag-list-field" %><%= f.text_field :ref, class: "hidden", :value => params[:ref] %>
			 <%= f.text_field :trend_list, class: "make-trends-field hidden-imp" %>
			 <%= f.check_box :anonymous, class: "hidden-imp", id: "created-meme-anonymous-field", checked: false %>
			 <%= f.text_field :trendname, class: "make-trends-field hidden-imp trendname" %>
			   <%= f.text_field :trendid, class: "make-trends-field hidden-imp trendid" %>

			 <%= f.submit :id => "submit-image-button-2" %>
			 <%# end %>
			 <div id="meme-textareas-wrapper">
			 	<div id="meme-textareas">
			 		<div class="memetextareas">
			 			<p>Top text</p>
						<textarea id="text-top" data-gramm="false" spellcheck="true"></textarea>
			 		</div>

			 		<div class="memetextareas">
			 			<p>Bottom text</p>
					<textarea id="text-bottom" data-gramm="false" spellcheck="true" ></textarea>
			 		</div>

					<div id="submit-button-before-wrapper">
						<div id="make-tags-2-wrapper" class="inline-make-tags-wrapper">
							<span>Tags</span>
							<p id="tag-memes" class="active-make-tag no-pointer">memes</p>
							<p id="tag-food">food</p>
							<p id="tag-photoshopped">photoshopped</p>
							<p id="tag-gaming">gaming</p>
							<p id="tag-sports">sports</p>
							<!--<p id="tag-animated">animated</p>-->
							<!--<p id="tag-cute">cute</p>-->
							<p id="tag-reaction">reaction</p>
							<p id="tag-wtf">wtf</p>
							<!--<p id="tag-politics">politics</p>-->
							<!--<p id="tag-rage-comic">rage comics</p>-->
							<!--<p id="tag-nsfw">nsfw</p>-->
							<p id="tag-happy">happy</p>
							<p id="tag-awesome">awesome</p>
							<p id="tag-opinion">opinion</p>
							<p id="tag-movies">movies</p>
						</div>

						<div class="make-trends-wrapper">
							<% @trends.any? %>
							<p class="make-trends-title">Trends</p>
		                      <% @trends.each do |trend| %>
		                      <p data-trend-name="<%= trend.name %>" class="make-trends">/<%= trend.name %></p>
		                    <% end %>
							<span class="make-add-trend-title">New trend</span>
							<input class="make-add-trend-input" placeholder="addTrendLikeThis">
							<span class="make-trend-notice">New trends are added like this.. vineIsDead</span>
						</div>

						<button id="created-image-button-before">Post</button>

						<a id="downloadImgLink" onclick="$('#downloadImgLink').attr('href', memeCanvas.toDataURL());" download="meme.png" href="#" target="_blank">
						<button id="cancel-upload-2" class="second"><i class="material-icons">file_download</i></button>
						</a>

						<button id="cancel-upload-2">cancel</button>

					<div id="make-tags-2" class="make-tags">
						<i class="material-icons">sort</i>
					</div>
					<div class="loading-icon "></div>
					<% if user_signed_in? && current_user.passed == true%>
						<span class="anonymous-checkbox shown">
					 	<input name="post[anonymous]" type="hidden" value="0"><input type="checkbox" value="1" name="post[anonymous]" id="post_anonymous">
					 	<i class="material-icons anonymous-checked"></i>
					 	<div id="anonymous-image" class="anonymous-image">
					 		<i class="material-icons">add</i>
					 	</div>
					 	<span>Anonymous</span>
					 	</span>
<% end %>



					</div>




				</div>

			 </div>

<% end %>
		</div>

		  <!--<div id="more-tools-wrapper">
			  <a id="id_download"><button class="tools" ><span class="eraser"></span></button></a>
