<!DOCTYPE html>
<html>
<head>
	
	<link href='https://fonts.googleapis.com/css?family=Raleway:900' rel='stylesheet' type='text/css'>
	<link href='https://fonts.googleapis.com/css?family=Roboto+Condensed' rel='stylesheet' type='text/css'>
</head>
<body id="<%= controller.action_name %>">	
	<span id="make-logo">/make</span>
	<div id="tabs-wrapper">
	  <div id="images-tab" class="tabs active-tab">Images</div>
	  <div id="memes-tab" class="tabs">Memes</div>
	  <div id="rage-comics-tab" class="tabs">Rage Comics</div>
	</div>
	<div id="memes-content" class="choosen-content">
		<div class="middle-column" id="meme-wrapper">
			<%= form_for @post, html: { multipart: true } do |f| %>
			 <input id="base64" type="hidden" value="" name="base64"/>
			 <canvas class="middle-column" id="meme-canvas" height="1100" width="1100"></canvas>
			 <%= f.text_area :title, id: "created-meme-title", placeholder: "Caption" %>
			 <%= f.text_field :facenumber,id: "created-meme-facenumber-field", class: "hidden anonymous-image-field" %>
			 <%= f.check_box :anonymous, class: "hidden-imp", id: "created-meme-anonymous-field", checked: false %>
			 
			 <%= f.submit :id => "submit-image-button-2" %>
			 <% end %>
			 <div id="meme-textareas-wrapper">
			 	<div id="meme-textareas">
			 		<div>
			 			<p>Top text</p>
						<textarea id="text-3" data-gramm="false" spellcheck="true"></textarea>
			 		</div>
			 		
			 		<div>
			 			<p>Bottom text</p>
					<textarea id="text-4" data-gramm="false" spellcheck="true" ></textarea>	
			 		</div>
			 		
					<div id="submit-button-before-wrapper"><button id="created-image-button-before">Post</button><button id="cancel-upload-2">cancel</button>
						<span class="anonymous-checkbox shown">
					 	<input name="post[anonymous]" type="hidden" value="0"><input type="checkbox" value="1" name="post[anonymous]" id="post_anonymous">
					 	<i class="material-icons anonymous-checked"></i>
					 	<div id="anonymous-image" class="anonymous-image">
					 		<i class="material-icons">add</i>
					 	</div>
					 	<span>Anonymous</span>
					 	</span>
					</div>
				</div>
			 </div>
			 
			 
		</div>
			
